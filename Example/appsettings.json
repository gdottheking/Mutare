{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*",
    "Kestrel": {
      "Endpoints": {
        "Http": {
          "Url": "http://0.0.0.0:5000",
          "Protocols": "Http1AndHttp2"
        }
      },
      "EndpointDefaults": {
        "Protocols": "Http1"
      }
    },
    "MetricsOptions": {
      "DefaultContextLabel": "ExampleApp",
      "Enabled": true,
      "ReportingEnabled": true,
      "GlobalTags": {}
    },
    "MetricsWebTrackingOptions": {
      "ApdexTrackingEnabled": true,
      "ApdexTSeconds": 0.1,
      "IgnoredHttpStatusCodes": [ 404 ],
      "IgnoredRoutesRegexPatterns": [],
      "OAuth2TrackingEnabled": true
    },
    "MetricEndpointsOptions": {
      "MetricsEndpointEnabled": true,
      "MetricsTextEndpointEnabled": true,
      "EnvironmentInfoEndpointEnabled": true
    },
    "___ignore__HttpsInlineCertAndKeyFile": {
      "___ignore__Url": "https://localhost:5001",
      "___ignore__Certificate": {
        "___ignore__Path": "./secrets/aspnet.pem",
        "___ignore__KeyPath": "./secrets/aspnet.key",
        "___ignore__Password": "S3cr3tPassw0rd"
      }
    }
  }
